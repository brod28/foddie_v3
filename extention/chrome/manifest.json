{
  "name": "foodie",
  "description": "foodie",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts":          [ {
    "js":       [ "injector.js" ],
    "matches":  [   "https://*/*"
    ]}],
  "icons": {"128": "icon_128.png"},
  "permissions": [
    "https://foodieforfoodie.herokuapp.com/*",
    "storage"
  ],
  "web_accessible_resources": ["foodie_script.js"]
}
